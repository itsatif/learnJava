import java.util.*;
public class HyperBit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub


		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		int count = 0;
		int temp = n;
		while(count<temp) {
			temp = temp/10;
			count++;
		}
		System.out.print(count);
	}
	
}


