import java.util.*;
public class HyperBit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub


		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		System.out.print(121/10);
		int val =0;
		
		while(n != 0) {
			int rev = n % 10;
			n = n/10;
			val = val*10+rev;
			}
			if(n == val) {
				System.out.print(val);
			}
			//else {
			//	System.out.print("not");
			//}
		
	}

}


