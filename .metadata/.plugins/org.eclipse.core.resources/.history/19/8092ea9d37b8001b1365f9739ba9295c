package beg;

public class reverseArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = {1,2,3,4,5};
		reverseArr(arr);

	}
	
	public static void reverseArr(int[] a) {
		for(int i=0;i<a.length/2;i++) {
			int temp = a[i];
			a[i] = a[a.length - i - 1];
			a[a.length - i - 1] = temp;
		}
	}

}
